#include <iostream>
using namespace std;

int main()
{
    /*int m[lines][columns] */
    int i; int j; int c; int l;
    cout<<"Insert l: "; cin>>l;
    cout<<"Insert c: "; cin>>c;
    
    int m[l][c];
    
    for(i=0; i<l; i++) {
        for(j=0; j<c; j++) {
            cout<<"m["<<i<<"]["<<j<<"]= "; cin>>m[i][j];
        }
    }
    
    int Me=m[1][0];
    
    for (int i=1; i<n; i++) {
        for(int j=0; j<i; j++) {//sub diag principala
            if(m[i][j]>Me) {
                Me=m[i][j];
            }
        }
    }
    cout<<"El. makim e "<<Me; return 0;
    
}